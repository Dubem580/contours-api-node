{
    "swagger": "2.0",
    "info": {
        "version": "1.5.0",
        "title": "Profile API"
    },
    "basePath": "/api/contours",
    "tags": [{
        "name": "profile",
        "description": "get profile data"
    }],
    "paths": {
        "/profile.{format}": {
            "get": {
                "tags": ["profile"],
                "summary": "Get Profile",
                "description": "Get Profile data based on latitude, longitude, azimuth, data source and unit of measurement. Response returned in JSON format.",
                "parameters": [{
                    "name": "lat",
                    "label": "Latitude",
                    "display": true,
                    "in": "query",
                    "description": "Latitude",
                    "required": true,
                    "type": "number",
                    "defaultValue": ""
                }, {
                    "name": "lon",
                    "label": "Longitude",
                    "display": true,
                    "in": "query",
                    "description": "Longitude",
                    "required": true,
                    "type": "number",
                    "defaultValue": ""
                }, {
                    "name": "azimuth",
                    "label": "Azimuth",
                    "display": true,
                    "in": "query",
                    "description": "Azimuth along which the profile is made.",
                    "required": true,
                    "type": "number",
                    "defaultValue": ""
                }, {
                    "name": "start",
                    "label": "Start",
                    "display": true,
                    "in": "query",
                    "description": "Distance of the starting point of profile from the location, in kilometers.",
                    "required": true,
                    "type": "number",
                    "defaultValue": ""
                }, {
                    "name": "end",
                    "label": "End",
                    "display": true,
                    "in": "query",
                    "description": "Distance of the ending point of the profile from the location, in kilometers.",
                    "required": true,
                    "type": "number",
                    "defaultValue": ""
                }, {
                    "name": "num_points",
                    "label": "No. of Points",
                    "display": true,
                    "in": "query",
                    "description": "Number of points used for the profile.",
                    "required": true,
                    "type": "number",
                    "defaultValue": ""
                }, {
                    "name": "src",
                    "label": "Source",
                    "display": true,
                    "in": "query",
                    "description": "Data source.",
                    "required": false,
                    "type": "string",
                    "defaultValue": "ned",
                    "enum": [                        
                        "ned_1",
                        "globe30"
                    ]
                }, {
                    "name": "unit",
                     "label": "Unit",
                    "display": true,
                    "in": "query",
                    "description": "Unit of measurement: m=meters, mi=miles, ft=feet",
                    "required": false,
                    "type": "string",
                    "defaultValue": "meters",
                    "enum": [
                    	"m",
                        "mi",
                        "ft"
                    ]
                }, {
                    "name": "format",
                    "label": "Format",
                    "display": false,
                    "in": "path",
                    "description": "Format for the returned results.<br /><br />Valid values: json.",
                    "required": true,
                    "type": "string",
                    "defaultValue": "json",
                    "enum": [
                        "json"
                    ]
                }],
                "responses": {
                    "200": {
                        "description": "OK"
                    }
                },
                "produces": [
                    "application/json"
                ]
            }
        }
    }
}
